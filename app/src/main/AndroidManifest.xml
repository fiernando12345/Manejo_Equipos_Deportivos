<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tec.fernandoalberto.manejoequipos">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icono"
        android:label="Torneo Voleibol"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="Equipos Voleibol"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Agregar_Equipos"
            android:label="Agregar Equipo"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".Editar_Equipo"
            android:label="Datos de Equipo"
            android:parentActivityName=".MainActivity"></activity>
    </application>

</manifest>